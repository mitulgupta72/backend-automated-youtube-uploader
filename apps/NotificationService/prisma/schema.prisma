datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Notification {
  createdAt DateTime                @default(now())
  id        String                  @id @default(cuid())
  message   String?
  recipient String?
  status    EnumNotificationStatus?
  updatedAt DateTime                @updatedAt
}

enum EnumNotificationStatus {
  Option1
}
